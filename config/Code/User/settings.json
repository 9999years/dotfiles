{
    "editor.fontFamily": "\"PragmataPro Liga\", monospace",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "editor.formatOnPaste": true,
    "editor.suggestSelection": "first",
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "debug.console.fontSize": 16,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/*.olean": true
    },
    "latex-workshop.hover.preview.newcommand.newcommandFile": "/home/becca/texmf/tex/latex/mathnotes/mathnotes.sty",
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.intellisense.unimathsymbols.enabled": true,
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex.lint.onSave": false,
    "python.formatting.provider": "black",
    "python.autoComplete.addBrackets": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=no-member,invalid-name,missing-function-docstring,missing-module-docstring,missing-class-docstring,trailing-newlines,bad-continuation,unsubscriptable-object,not-an-iterable"
    ],
    "python.linting.pydocstyleArgs": [
        "--convention=pep257",
        "--add-ignore=D105,D200"
    ],
    "rust.clippy_preference": "on",
    "rust.rustflags": "-A dead_code -A unused_imports",
    "rust.racer_completion": false,
    "rust-client.updateOnStartup": true,
    "rust-analyzer.displayInlayHints": true,
    "rust-analyzer.trace.server": "verbose",
    "rust-analyzer.highlightingOn": true,
    "rust-analyzer.inlayHints.chainingHints": false,
    "rust-analyzer.inlayHints.typeHints": false,
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "matklad.rust-analyzer"
    },
    "[json]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": false,
        "editor.defaultFormatter": "ms-python.python"
    },
    "[javascript]": {
        "editor.formatOnSave": true
    },
    "[cpp]": {
        "editor.formatOnSave": true
    },
    "[nix]": {
        "editor.formatOnSave": true
    },
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.hlsearch": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.rendererType": "experimentalWebgl",
    "update.mode": "none",
    "vsicons.dontShowNewVersionMessage": true,
    "window.zoomLevel": 1,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "material-icon-theme.hidesExplorerArrows": false,
    "material-icon-theme.folders.color": "#90a4ae",
    "vsicons.presets.foldersAllDefaultIcon": true,
    "explorer.confirmDelete": false,
    "workbench.colorTheme": "Noctis High Contrast",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "explorer.confirmDragAndDrop": false,
    "java.semanticHighlighting.enabled": true,
    "debug.onTaskErrors": "abort",
    "java.requirements.JDK11Warning": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.suggest.statusBar.visible": true
}
